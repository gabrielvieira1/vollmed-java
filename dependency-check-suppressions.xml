<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">

    <!-- 
    Arquivo de supressões para o OWASP Dependency-Check
    Este arquivo permite suprimir falsos positivos ou vulnerabilidades 
    aceitas conscientemente para fins educacionais.
    -->
    
    <!-- Supressões para dependências de teste (se aplicável) -->
    <suppress>
        <notes><![CDATA[
            Vulnerabilidades em dependências de teste não afetam produção
        ]]></notes>
        <filePath regex="true">.*test.*\.jar</filePath>
    </suppress>
    
    <!-- Supressões para Spring Boot DevTools (desenvolvimento apenas) -->
    <suppress>
        <notes><![CDATA[
            Spring Boot DevTools é usado apenas em desenvolvimento
        ]]></notes>
        <filePath regex="true">.*spring-boot-devtools.*\.jar</filePath>
    </suppress>
    
